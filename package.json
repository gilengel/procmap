    "quasar": "^2.0.0",
    "rete": "^1.4.5-rc.1",
    "rete-connection-plugin": "^0.9.0",
    "rete-dock-plugin": "^0.2.1",
    "string-similarity": "^4.0.4",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.10",
    "vue-grid-layout": "git+https://github.com/lvjunhao/vue-grid-layout.git",
    "vuedraggable": "^4.0.3",
    "vuex": "^4.0.1",
    "vuex-module-decorators": "^1.0.1"
